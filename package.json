{
  "name": "@johnpc/circleci-aws-token-rotation",
  "version": "1.0.0",
  "description": "Scheduled job to rotate AWS tokens in CircleCI",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "preinstall": "(command -v git && git config --local commit.template \".commit-msg\") || true"
  },
  "author": {
    "name": "John Corser",
    "url": "https://jpc.io",
    "organization": false
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/assert": "^1.109.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.7.8",
    "esbuild": "^0.12.9",
    "husky": "^3.0.5",
    "jest": "^26.0.23",
    "lint-staged": "^9.2.5",
    "prettier": "^1.13.7",
    "ts-jest": "^26.0.23",
    "ts-node": "^10.0.0",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@aws-cdk/aws-events": "^1.65.0",
    "@aws-cdk/aws-events-targets": "^1.65.0",
    "@aws-cdk/aws-lambda": "^1.65.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.109.0",
    "@aws-cdk/aws-s3-assets": "^1.65.0",
    "@aws-cdk/core": "^1.65.0",
    "aws-cdk": "*",
    "aws-sdk": "^2.930.0",
    "axios": "^0.21.1",
    "fs-extra": "^10.0.0",
    "jest-mock-axios": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md,scss,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
